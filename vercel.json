{
    "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "./",
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/about", "dest": "/about" },
    { "src": "/product", "dest": "/product" },
    { "src": "/Galleryy", "dest": "/Galleryy" },
    { "src": "/contact", "dest": "/contact" }
  ]
}
