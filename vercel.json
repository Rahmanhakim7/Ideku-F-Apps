{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "./",
  "routes": [
    { "src": "/about", "dest": "/about" },
    { "src": "/#product", "dest": "/product" },
    { "src": "/#gallery", "dest": "/gallery" },
    { "src": "/#contact", "dest": "/contact" },
    { "src": "/", "dest": "/index.html" }
  ]
}
